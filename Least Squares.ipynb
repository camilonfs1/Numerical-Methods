   "source": [
    "F=np.array([\n",
    "   [1, 0    ,0   ], \n",
    "   [1, 0.7  ,0.49], \n",
    "   [1, 1.5  ,2.25], \n",
    "   [1, 2.2  ,4.84],\n",
    "   [1, 3    ,9   ]]) \n",
    "\n",
    "y=[0,6,12,7,2]\n",
    "\n",
    "FT = F.transpose()\n",
    "S=np.dot(FT, F)\n",
    "\n",
    "b=np.dot(FT, y)\n",
    "x = np.linalg.inv(S)\n",
    "c= np.dot(x, b)\n",
    "\n",
    "print(c)\n"
   ]
